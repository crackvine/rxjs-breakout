{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "no-console": 0,
      "no-nested-ternary": 0,
      "no-plusplus": 0,
      "max-len": ["error", { "code": 120}]
    }
}
